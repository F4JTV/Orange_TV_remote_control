MainWindow {
    background-color: black
}

QPushButton#orange {
    color: white;
    background-color: grey;
    border-style: outset;
    border-width: 2px;
    border-color: black;
    border-radius : 25;
    border : 2px solid black
}

QPushButton:hover#orange {
    color: white;
    background-color: darkgrey;
    border-style: outset;
    border-width: 2px;
    border-color: black;
    border-radius : 25;
    border : 2px solid black
}

QPushButton:pressed#orange {
    color: black;
    background-color: white;
    border-style: outset;
    border-width: 2px;
    border-color: white;
    border-radius : 25;
    border : 2px solid white
}